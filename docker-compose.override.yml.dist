version: '3'

services:
  jtrader:
    build:
      context: ./.docker
      dockerfile: Dockerfile-development
    volumes:
      - ./:/src
    privileged: true
    command:
      - "jtrader"
      - "-d"
    environment:
      - AWS_DEFAULT_REGION=us-west-2
      - AWS_ACCESS_KEY_ID=xXxX
      - AWS_SECRET_ACCESS_KEY=xXxX
